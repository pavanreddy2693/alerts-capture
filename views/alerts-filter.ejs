<!DOCTYPE html>
<html>
  <head>
    <title>Alerts-Capture: Alerts-Filtered</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
  <link rel="stylesheet" href="/stylesheets/style.css"/>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"/>

  <style>
    .nav-link,.navbar-brand{
      color:white;
    }
    .nav-link:hover,.navbar-brand:hover{
      color:white;
    }
    .text-teal{
      color: teal;
    }
    .bg-teal{
      background-color: teal;
      color: white;
    }
    .btn:hover{
      background-color: white;
      color: teal;
      box-shadow: 0 0 10px teal;
    }
   .navbar{
    background-color:teal;
   }
   .navbar:hover{
     background-color: teal;
     color:black;
   }
 
  </style>
</head>

  <body>


   
    <nav class="navbar navbar-expand-lg">
      <div class="container-fluid">
        <a class="navbar-brand" href="/errors/all">Alerts Capture</a>
        <button class="navbar-toggler bg-light text-teal" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <i class="fa fa-bars"></i>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
            <li class="nav-item"><a href="/add" class="nav-link">Add Alert</a></li>
            <li class="nav-item"> <a href="/errors/all" class="nav-link active">View Alerts</a></li>
            <li class="nav-item"> <a href="/errors/triggered" class="nav-link">5xx API</a></li>
            
            <li> <a href="/users/logout" class="nav-link">logout</a></li>
            
          </ul>
        </div>
      </div>
    </nav>

    <% include ./partials/messages %>


    
   
    <div style="align-items: center; font-size: 12px;">
      <form action="/alerts/filter" method="GET" class="row form">
        <span class="text-teal mx-2  my-1" style="font-size: medium;"><b>Filters</b></span>
        <div class="col-sm-12 col-md-12 col-lg-4 my-1">
          <div class="row">

          <div class="col-sm-12 col-md-12 col-lg-6 my-1">        
            <input list="alerttypes" name="type" placeholder="Alert Type" class="form-control">
              <datalist id="alerttypes" >
              <option>4xx</option>
              <option>5xx</option>
              <option>ALARM</option>
              <option>backend-infra</option>
              <option>check-apdex-score-supply: Critical</option>
              <option>CPU-High</option>
              <option>cpuUsage</option>
              <option>CPUUtilization</option>
              <option>CurrConnections</option>
              <option>ES_4xx</option>
              <option>ES_5xx</option>
              <option>HealcheckFailing</option>
              <option>healthyHost</option>
              <option>InstanceDown</option>
              <option>KibanaHealthyNodes</option>
              <option>OutOfDiskSpace</option>
              <option>OutOfMemory</option>
              <option>RDS_Connections</option>
              <option>readIOPS</option>
              <option>sensu</option>
              <option>TargetResponseTime</option>
              <option>UnhealthyHost</option>
              <option>WriteIOPS</option>
              <option>FreeableMemory</option>
              <option>EngineCPUUtilization</option>
              <option>NetworkByteIn+NetworkByteOut</option>
              <option>kafka</option>
              <option>ClusterStatus</option>
              <option>apache-thread-counts</option>
              <option>IOPS</option>
              <option>Unhealthy_Or_NotInTg</option>
              <option>freeStorage</option>
              <option>ReplicaLagAlert</option>
              <option>cloudfront_requests</option>
              <option>cloudfront_total_error_rate</option>
              <option>Cloudfront_5xx</option>
              <option>Cloudfront_4xx</option>
              <option>mongodb_Members_Manifest</option>
              <option>HDFSUtilization</option>
              <option>MRUnhealthyNodes</option>
             <option>MultiMasterInstanceGroupNodesRunning</option>
             <option>MissingBlocks</option>
             <option>CorruptBlocks</option>
             <option>UnderReplicatedBlocks</option>
             <option>data-infra-alerts</option>
             <option>KafkaConnectAlert</option>
             <option>Replication_Error</option>
             <option>Nodes</option>
             <option>ActiveControllerCount</option>
             <option>OfflinePartitionsCount_0+</option>
             <option>CpuUser</option>
             <option>KafkaDataLogsDiskUsed</option>
             <option>DatabaseMemoryUsagePercentage</option>
             <option>Redis-wallet_schedule_queue</option>
             <option>KafkaDataLogsDiskUsed</option>
             <option>ReplicationLag</option>
             <option>JVMMemoryPressure</option>
             <option>Connection-Errors</option>
            
              </datalist>
      </div>

       
      <div class="col-sm-12 col-md-12 col-lg-6">
          <input type="text" class="d-inline-block  form-control  my-1" placeholder="Alert Name" name="alertname"/>
        </div>
        </div>
        </div>
    
         
      <div class="d-inline col-sm-12 col-md-12 col-lg-3  my-1">
        <div class="row">
          <div class="col-sm-12 col-md-12 col-lg-4">
            <span class="d-inline-block  my-1" style="font-size: 14px;">DateFrom</span>
          </div>
          <div class="col-sm-12 col-md-12 col-lg-8">
            <input type="date" name="datefrom"  class="my-1 form-control"/>
          </div>
        </div>
      </div>

      
      <div class="d-inline col-sm-12 col-md-12 col-lg-3  my-1">
        <div class="row">
          <div class="col-sm-12 col-md-12 col-lg-4">
            <span class=" my-1" style="font-size: medium;">DateTo</span>
          </div>
          <div class="col-sm-12 col-md-12 col-lg-8">
            <input type="date" name="dateto" class="d-inline-block form-control my-1" />
          </div>
        </div>
        
       
    </div>
        
      <div class="d-inline col-sm-12 col-md-12 col-lg-2  my-1">
        <input type="submit" class="d-inline-block bg-teal  btn my-1" value="Summarise"/>
    </div>
    </form>
    </div>
      
   

    <div>
      <table class="table m-auto">
        <thead>
          <th style="width: 15%;">alert name</th>
          <th style="width: 15%;">alert type</th>
          <th style="width: 30%;">Dev Response</th>
          <th style="width: 30%;">Count</th>
          
          
          

        </thead>
        <tbody>
          <tbody>

            <% filterbydate.forEach((filterbyd) => { %>
              <tr>
              <td style="width: 15%;"><%= filterbyd._id.alertname %></td>
              <td style="width: 15%;"><%= filterbyd._id.alerttype %></td>
              <td style="width: 30%;"><%= filterbyd._id.dev_response %></td>
              <td style="width: 40%;"><%= filterbyd.count %></td>
             
            </tr>
            <% });%>
              </tbody>
        </tbody>
      </table>

      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW" crossorigin="anonymous"></script>

  </body>
</html>
